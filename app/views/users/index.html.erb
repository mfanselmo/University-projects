<h1>Usuarios existentes</h1>

<% @users.each do |user| %>
  <div class="container">
    <div class="row">
      <div class="col-sm-12"></div>
    </div>
    <div class="row">
      <div class="col-sm-1">
        <div class="thumbnail">
			<%= image_tag(user.avatar.url , :alt => "user view") %>
        </div>
      </div>
      <div class="col-sm-10">
        <div class="panel panel-default">
          <div class="panel-heading">
            <strong><%= user.username %></strong>
        </div>
      </div>
    </div>
  </div>
 </div>
<% end %>